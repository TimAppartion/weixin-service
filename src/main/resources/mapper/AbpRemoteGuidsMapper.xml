<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.jiuzhou.user.mapper.AbpRemoteGuidsMapper">
    <insert id="insertOne">
        INSERT INTO AbpRemoteGuids
        ( BusinessDetailGuid,CreationTime,CreatorUserId,IsActive,BerthsecId,DeviceCode,EmployeeId,UpdateTime )
        VALUES
        ( #{query.BusinessDetailGuid},#{query.CreationTime},#{query.CreatorUserId},#{query.IsActive},#{query.BerthsecId},#{query.DeviceCode},#{query.EmployeeId},#{query.UpdateTime} )
    </insert>
</mapper>
