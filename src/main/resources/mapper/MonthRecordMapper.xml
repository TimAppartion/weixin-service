<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.jiuzhou.user.mapper.MonthRecordMapper">
    <insert id="insertOne">
        INSERT INTO MonthRecord
        ( openId,plateNumber,monthly_total_fee,parkId,month,monthlyType,uid )
        VALUES
        ( #{query.openId},#{query.plateNumber},#{query.monthly_total_fee},#{query.parkId},#{query.month},#{query.monthlyType},#{query.uid} )
    </insert>
    <select id="getById" resultType="com.example.jiuzhou.user.model.MonthRecord">
        select * from MonthRecord where id = #{id}
    </select>
</mapper>
