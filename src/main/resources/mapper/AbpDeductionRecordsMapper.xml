<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.jiuzhou.user.mapper.AbpDeductionRecordsMapper">

    <insert id="insetOne">
        INSERT INTO AbpDeductionRecords
        ( OtherAccountId,OperType,Money,PayStatus,InTime,Remark,CardNo,EmployeeId,UserId,PlateNumber,TenantId,CompanyId,BeginMoney,EndMoney )
        VALUES
        ( #{query.OtherAccountId},#{query.OperType},#{query.Money},#{query.PayStatus},#{query.InTime},#{query.Remark},#{query.CardNo},
        #{query.EmployeeId},#{query.UserId},#{query.PlateNumber},#{query.TenantId},#{query.CompanyId},#{query.BeginMoney},#{query.EndMoney})
    </insert>
</mapper>

