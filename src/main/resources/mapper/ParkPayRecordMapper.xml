<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.jiuzhou.user.mapper.ParkPayRecordMapper">
    <insert id="insertOne">
        INSERT INTO ParkPayRecord
        ( TenantId,Money,ParkadeAccessId,OpenId,UserId,PlateNumber,CreationTime ,transaction_id)
        VALUES
        ( #{query.TenantId},#{query.Money},#{query.ParkadeAccessId},#{query.OpenId},#{query.UserId},#{query.PlateNumber},#{query.CreationTime},#{query.transactionId} )
    </insert>
    <select id="getByTransactionId" resultType="com.example.jiuzhou.user.model.ParkPayRecord">
        select * from ParkPayRecord
        where
        transaction_id = #{transactionId}
    </select>
</mapper>
